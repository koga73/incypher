		"rimraf": "^4.0.5"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/koga73/incypher.git"
	},
	"keywords": [
		"crypto",
		"seed",
		"pass",
		"encrypt",
		"decrypt",
		"archive",
		"zip",
		"secure",
		"store",
		"rclone",
		"cloud",
		"sync"
	],
	"bugs": {
		"url": "https://github.com/koga73/incypher/issues"
